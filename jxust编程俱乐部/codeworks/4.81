#include<iostream>
#include<iomanip>
#define P1 3.14
using namespace std;
int main()
{
	float r,a,b,c,d,e;
	float h;
	cin>>r>>h;
	a=2*r*P1;
	b=r*r*P1;
	c=4*r*r*P1;
