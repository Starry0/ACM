#include<iostream>
using namespace std;
int main()
{
	char str1[30],str2[30],*p1,*p2;
	int i,j,n,m;
	p1=str1;p2=str2;
	void copy(char *,char *,int ,int );
	cin>>n;
	for(i=0;i<n;i++)
	cin>>*(p1+i);
	cin>>m;
	copy(p1,p2,m,n);
	for(i=0;i<=n-m+1;i++)
	cout<<*(p2+i);
	return 0;
}
void copy(char *p1,char *p2,int m,int n)
{
	for(int i=0;i<=n-m+1;i++)
	*(p2+i)=*(p1+m-1+i);
}